{
	"name": "keyvaultpipeline",
	"properties": {
		"activities": [
			{
				"name": "KeyvaultWeb",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": {
						"value": "@https://@{pipeline().parameters.p_keyvaultname}.vault.azure.net/secrets/@{pipeline().parameters.p_secretinfo}?api-version=7.0",
						"type": "Expression"
					},
					"authentication": {
						"type": "MSI",
						"resource": "https://vault.azure.net"
					}
				}
			}
		],
		"parameters": {
			"p_keyvaultname": {
				"type": "string",
				"defaultValue": "adfinterview"
			},
			"p_secretinfo": {
				"type": "string",
				"defaultValue": "adfinterview/59bfc9bfcbb6422689ace2b0759a6d12"
			}
		},
		"annotations": []
	}
}